"line":80,"column":9}},{"generated":{"line":91,"column":20},"source":"js/list.js","original":{"line":80,"column":4}},{"name":"innerHTML","generated":{"line":91,"column":21},"source":"js/list.js","original":{"line":80,"column":19}},{"generated":{"line":91,"column":30},"source":"js/list.js","original":{"line":80,"column":4}},{"generated":{"line":91,"column":33},"source":"js/list.js","original":{"line":80,"column":31}},{"generated":{"line":91,"column":35},"source":"js/list.js","original":{"line":80,"column":4}},{"generated":{"line":92,"column":0},"source":"js/list.js","original":{"line":81,"column":3}}],"sources":{"js/list.js":"import {Content} from './content'\r\n\r\nexport class List {\r\n  constructor(container, data) {\r\n    this.container = container;\r\n    this.data = data;   \r\n    this.activeItemId = null;\r\n    \r\n    this.handleClickList = this._clickList.bind(this)\r\n    this._init();\r\n  }\r\n\r\n  _init() {\r\n    this._render(this.data)\r\n    this.container.addEventListener('click', this.handleClickList)\r\n  }\r\n\r\n  _removeActive() {\r\n    if (!this.activeItemId) return;\r\n\r\n    const tag = this.container.querySelector(`[data-id=\"${this.activeItemId}\"]`)  \r\n     \r\n    if (!tag == null) tag.classList.remove('active');   \r\n    \r\n    tag.classList.remove('active');\r\n  }\r\n\r\n  _selectListItem(id) {\r\n    \r\n    this._removeActive()\r\n\r\n    this.activeItemId = id\r\n    \r\n    const tag = this.container.querySelector(`[data-id=\"${id}\"]`)   \r\n    tag.classList.add('active')\r\n\r\n    fetch( '/api/data', { method: 'GET' })\r\n      .then(function(response) {\r\n        return response.json()\r\n      })\r\n      .then(function(dataObj) {\r\n        dataObj.list.forEach(function(item) {\r\n          if (item.id == id) {\r\n            new Content(document.querySelector('#content'), item)\r\n          } \r\n        })      \r\n      })\r\n      .catch((error) => console.error(error)) \r\n  }\r\n\r\n\r\n\r\n  _clickList() {\r\n    const tag = event.target\r\n    \r\n    if (tag.classList.value.includes(\"list-item\")) {\r\n      const id = tag.getAttribute('data-id')  \r\n      \r\n      this._selectListItem(id)            \r\n    }   \r\n  }\r\n\r\n  _render(data) {    \r\n    this._clear()\r\n    this.data.forEach((item) => {\r\n      \r\n      const template = `\r\n        <div class=\"list-item p-3\" data-id=\"${item.id}\">\r\n          <h5>${item.title}</h5> \r\n          <small>${item.date}</small> \r\n        </div>        \r\n      `;\r\n      \r\n      this.container.innerHTML = this.container.innerHTML + template;\r\n    })\r\n\r\n  }  \r\n\r\n  _clear() {\r\n    this.container.innerHTML = \"\";\r\n  }\r\n\r\n}"},"lineCount":null}},"error":null,"hash":"0cbee0270dca1142ea54d58dda06404b","cacheData":{"env":{}}}